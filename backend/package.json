{
    "name": "softwareproject-backend",
    "version": "1.0.0",
    "description": "Template for the softwareproject lecture at the University of Konstanz, Germany",
    "private": true,
    "scripts": {
        "build": "tsc && tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "start": "npm run build && node dist/src/server.js",
        "lint": "eslint \"**/*.{js,ts}\"",
        "test": "npm run build && jasmine --config=jasmine.json",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"tsc -w\" \"nodemon --inspect dist/src/server.js\""
    },
    "dependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/csvtojson": "^2.0.0",
        "@types/jspdf": "^2.0.0",
        "@types/qrcode": "^1.5.0",
        "@types/sequelize": "^4.28.15",
        "@types/sinon": "^10.0.15",
        "@types/uid-safe": "^2.1.2",
        "@types/xml2js": "^0.4.11",
        "bcrypt": "^5.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "csvtojson": "^2.0.10",
        "errorhandler": "1.5.1",
        "express": "4.18.2",
        "fast-levenshtein": "^3.0.0",
        "fs": "^0.0.1-security",
        "jspdf": "^2.5.1",
        "pg": "^8.10.0",
        "pg-hstore": "^2.3.4",
        "qrcode": "^1.5.3",
        "sequelize": "^6.32.0",
        "sequelize-typescript": "^2.1.5",
        "sinon": "^15.2.0",
        "source-map-support": "0.5.21",
        "uid-safe": "^2.1.5",
        "xml2js": "^0.6.0"
    },
    "devDependencies": {
        "@types/cors": "2.8.13",
        "@types/errorhandler": "1.5.0",
        "@types/eslint": "8.37.0",
        "@types/express": "^4.17.17",
        "@types/jasmine": "4.3.1",
        "@types/source-map-support": "0.5.6",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.57.1",
        "@typescript-eslint/parser": "5.57.1",
        "concurrently": "8.0.1",
        "eslint": "8.37.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "jasmine": "4.6.0",
        "nodemon": "2.0.22",
        "supertest": "6.3.3",
        "typescript": "5.0.3"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
